            drawKina = 0;
            with objPlayer{event_user(2);}
            wAni = instance_create(x,y+kinaY,oPlayerWarpFade);
            wAni.sprite_index = sprKinaHead;
            wAni.image_index = 0;
            wAni.image_xscale = -1;
            scrPlaySoundExt(sndAwaken,0.8,1.5,0);
            scrPlaySoundExt(sndSelectMade,1,.8,0);
            audio_sound_gain(global.currentMusic,0,1000);
        }
        if t == 100{
            frontA = min(frontA+1/100,1);
            if frontA == 1{state = 3;instance_create(400,304,oIntroTextBox)}
        }
    break;
    case 3://nothing
        
    break;
}

kinaA = (kinaA+1) mod 360;
kinaY = round(3*sin(degtorad(kinaA)));
</string>
