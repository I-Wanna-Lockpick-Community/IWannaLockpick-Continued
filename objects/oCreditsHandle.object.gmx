    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>switch state{
    case 1://fade in
        fadeA = max(fadeA - 1/250, 0);
    break;
    case 2://fade out
        fadeA = min(fadeA + 1/150, 1);
        if fadeA == 1{
            t = min(t+1,100);
            if t == 100{
                ///Return to title
                global.autosave = 0;
                global.inCutscene = 0;
                scrRestartGame();
            }
        }
    break;
}
if doScroll{
    scrollY = min(scrollY + scrollSpd, yMax);
    if scrollY == yMax{
        doScroll = 0;
        reachedEnd = 1;
    }
}
if reachedEnd{
    if (scrButtonCheckPressed(global.selButton) || scrButtonCheckPressed(global.jumpButton)) &amp;&amp; state != 2{
        state = 2;
        scrPlaySoundExt(sndSelectMade,.7,1,false);
        audio_sound_gain(global.currentMusic,0,3000);
    }
}else{//Skip
    if (scrButtonCheckPressed(global.selButton) || scrButtonCheck(global.jumpButton)) &amp;&amp; doScroll{
        scrollY = min(scrollY + scrollSpd*7, yMax);
    }
    if scrButtonCheckPressed(global.skipButton){
        doScroll = 0;
        state = 1;
        fadeA = 0;
        reachedEnd = 1;
        scrollY = yMax;
        scrPlaySoundExt(sndSelectMade,.7,1,false);
    }
}
titleA = (titleA + 1) mod 360;
titleY = 16*sin(degtorad(titleA));

background_y[0] = round(-scrollY*.05);
background_y[1] = round(-scrollY*.25);
background_y[2] = round(-scrollY*.5);
</string>
          </argument>
        </arguments>
      </action>
