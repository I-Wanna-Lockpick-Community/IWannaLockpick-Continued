        <arguments>
          <argument>
            <kind>1</kind>
            <string>if !validRoom{exit;}
if !instance_exists(objPlayer){exit;}
if global.inCutscene{exit;}

if saveBuffered{
    stableFooting = 0;
    if instance_exists(objPlayer){
        with oDoorSimple{solid = 0;}
        with oDoorCombo{solid = 0;}
        with objPlayer{//Don't save if near a door or off the ground (important for n-masters)
            if !place_free(x,y+1) &amp;&amp; place_free(x,y){
                other.stableFooting = 1;
                //RGB auras
                if global.key[key_RED] - global.key[key_MAROON] &gt;= 1 &amp;&amp; global.key[key_RED] &gt; 0{aura[0] = 1;}else if global.key[key_RED] - global.key[key_MAROON] &lt;= -1 &amp;&amp; global.key[key_MAROON] &gt; 0{aura[0] = -1;}else{aura[0] = 0;}
                if global.key[key_GREEN] - global.key[key_FOREST] &gt;= 5 &amp;&amp; global.key[key_GREEN] &gt; 0{aura[1] = 1;}else if global.key[key_GREEN] - global.key[key_FOREST] &lt;= -5 &amp;&amp; global.key[key_FOREST] &gt; 0{aura[1] = -1;}else{aura[1] = 0;}
                if global.key[key_BLUE] - global.key[key_NAVY] &gt;= 3 &amp;&amp; global.key[key_BLUE] &gt; 0{aura[2] = 1;}else if global.key[key_BLUE] - global.key[key_NAVY] &lt;= -3 &amp;&amp; global.key[key_NAVY] &gt; 0{aura[2] = -1;}else{aura[2] = 0;}
                if global.key[key_BROWN] &gt;= 1{
                    brownMode = 1;
                }else if global.key[key_BROWN] &lt;= -1{
                    brownMode = -1;
                }else{
                    brownMode = 0;
                }
                if instance_exists(oDoorSimple){
                    var dist = distance_to_object(oDoorSimple);
                    if dist &lt;= 1{
                        other.stableFooting = 0;
                    }
                    if dist &lt;= 23 &amp;&amp; (brownMode != 0 || aura[0] == 1 || aura[1] == 1 || aura[2] == 1){
                        other.stableFooting = 0;
                    }
                }
                if instance_exists(oDoorCombo){
                    var dist = distance_to_object(oDoorCombo);
                    if dist &lt;= 1{
                        other.stableFooting = 0;
                    }
                    if dist &lt;= 23 &amp;&amp; (brownMode != 0 || aura[0] == 1 || aura[1] == 1 || aura[2] == 1){
                        other.stableFooting = 0;
                    }
                }
                if instance_exists(oKeyBulk){
                    if place_meeting(x,y,oKeyBulk){
                        other.stableFooting = 0;
                    }
                }
                if instance_exists(oGate){
                    if place_meeting(x,y,oGate){
                        other.stableFooting = 0;
                    }
                }
                if instance_exists(oKina){
                    var dist = distance_to_object(oKina);
                    if dist &lt;= 1{
                        other.stableFooting = 0;
                    }
                    if dist &lt;= 23 &amp;&amp; brownMode != 0{
                        other.stableFooting = 0;
                    }
                }
            }
            if place_meeting(x,y,oSalvageIn){
                other.stableFooting = 0;
            }
        }
        with oDoorSimple{solid = 1;if brownNearPlayer{other.stableFooting = 0;}}
        with oDoorCombo{solid = 1;if brownNearPlayer{other.stableFooting = 0;}}
    }
    if stableFooting{
        //SAVE A GAME STATE TO THE UNDO STACK
        saveBuffered = 0;
        undoPUSH();
    }else{
        show_debug_message("Not stable footing");
    }
}

//Disable undo functionality while the camera is in freecam mode
if instance_exists(oNewCamera){
    if oNewCamera.freeCam == 1{
        exit;
    }
}

if scrButtonCheckPressed(global.shootButton) &amp;&amp; global.pmState == 0{
    if undoPos == 1{
        if !recentPush{
            undoPOP();
            undoPUSH();
            scrPlaySoundExt(sndSelectBeep,.6,.6,0);
        }
    }else if undoPos &gt;= 1{
        if recentPush{
            if saveBuffered{
                undoPOP();
                undoPUSH();//Re-saves the loaded state
            }else{
                undoPOP();
                undoPOP();
                undoPUSH();//Re-saves the loaded state
                saveBuffered = 0;
            }
        }else{
            undoPOP();
            undoPOP();
            undoPUSH();
            saveBuffered = 0;
        }
        scrPlaySoundExt(sndSelectBeep,.6,.6,0);
        //Push at the start to save the initial state
        //Push for any new action
        //If an action is about to be saved, reload the previous state and re-push it.
        //If an action has already been done, go two steps back and re-push.
        //If going on a rewind streak, pop both the "current" state and "one behind it" state, then push the latter again.
    }
    if undoPos == 0 &amp;&amp; ds_stack_size(undoStack) == 0{//If you've undone to the beginning, re-save the initial state
        undoPUSH();
    }
}

with oDoorSimple{
    undoReposition();
}
with oKeyBulk{
    undoReposition();
}
with oDoorCombo{
    undoReposition();
}
